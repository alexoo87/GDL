!-----------------------------------------------------------------------------------------------
"cutPlane":
!-----------------------------------------------------------------------------------------------
	_hPos = depth + 0.020
	_nCutPlane = 0
	addz	_dumpValue
		!	1st
		_xLength_1 = moX01
		_yLength_1 = moY01
		_zLength_1 = moZ01

		_xyLength_1 = sqr(_xLength_1^2 + _yLength_1^2)
		_xyzLength_1 = sqr(_xyLength_1^2 + _zLength_1^2)

		if	_xLength_1 < eps	then
			_aHorizon_1 = -90
		else
			_aHorizon_1 = atn(_yLength_1/_xLength_1)
		endif
		_aVertical_1 = atn(_zLength_1/_xyLength_1)

		if	_bCutPlane_1	then
			rotz	_aHorizon_1
				roty	-_aVertical_1
					addz	cutLct_1
						rotx	90 - aCut_1
							cutplane	1,	1,	0,	0	:	_nCutPlane = _nCutPlane + 1
							! gosub	"testPlane"
						del 1
					del 1
				del 1
			del 1
		endif

		!	2nd
		_xLength_2 = abs(moX02 - moX01)
		_yLength_2 = moY02 - moY01
		_zLength_2 = moZ02 - moZ01

		_xyLength_2 = sqr(_xLength_2^2 + _yLength_2^2)
		_xyzLength_2 = sqr(_xyLength_2^2 + _zLength_2^2)

		if	_xLength_2 < eps	then
			_aHorizon_2 = 90
		else
			_aHorizon_2 = atn(_yLength_2/_xLength_2)
		endif
		_aVertical_2 = atn(_zLength_2/_xyLength_2)

		if	_bCutPlane_2	then
			add	moX01,	moY01,	moZ01
				rotz	180 - _aHorizon_2
					roty	-_aVertical_2
						addz	cutLct_2
							rotx	90 - aCut_2
								cutplane	1,	1,	0,	0	:	_nCutPlane = _nCutPlane + 1
								!gosub	"testPlane"
							del 1
						del 1
					del 1
				del 1
			del 1
		endif

		!	3rd
		_xLength_3 = abs(moX03 - moX02)
		_yLength_3 = moY03 - moY02
		_zLength_3 = moZ03 - moZ02

		_xyLength_3 = sqr(_xLength_3^2 + _yLength_3^2)
		_xyzLength_3 = sqr(_xyLength_3^2 + _zLength_3^2)

		if	_xLength_3 < eps	then
			_aHorizon_3 = 90
		else
			_aHorizon_3 = atn(_yLength_3/_xLength_3)
		endif
		_aVertical_3 = atn(_zLength_3/_xyLength_3)

		if	_bCutPlane_3	then
			add	moX02,	moY02,	moZ02
				rotz	180 - _aHorizon_3
					roty	-_aVertical_3
						addz	cutLct_3
							rotx	90 - aCut_3
								cutplane	1,	1,	0,	0	:	_nCutPlane = _nCutPlane + 1
								!gosub	"testPlane"
							del 1
						del 1
					del 1
				del 1
			del 1
		endif

		!	4th
		_xLength_4 = abs(moX04 - moX03)
		_yLength_4 = moY04 - moY03
		_zLength_4 = moZ04 - moZ03

		_xyLength_4 = sqr(_xLength_4^2 + _yLength_4^2)
		_xyzLength_4 = sqr(_xyLength_4^2 + _zLength_4^2)

		if	_xLength_4 < eps	then
			_aHorizon_4 = -90
		else
			_aHorizon_4 = atn(_yLength_4/_xLength_4)
		endif
		_aVertical_4 = atn(_zLength_4/_xyLength_4)

		if	_bCutPlane_4	then
			add	moX03,	moY03,	moZ03
				rotz	180 - _aHorizon_4
					roty	-_aVertical_4
						addz	cutLct_4
							rotx	90 - aCut_4
								cutplane	1,	1,	0,	0	:	_nCutPlane = _nCutPlane + 1
								!gosub	"testPlane"
							del 1
						del 1
					del 1
				del 1
			del 1
		endif

		!	5th
		_xLength_5 = abs(-moX04)
		_yLength_5 = -moY04
		_zLength_5 = -moZ04

		_xyLength_5 = sqr(_xLength_5^2 + _yLength_5^2)
		_xyzLength_5 = sqr(_xyLength_5^2 + _zLength_5^2)

		if	_xLength_5 < eps	then
			_aHorizon_5 = -90
		else
			_aHorizon_5 = atn(_yLength_5/_xLength_5)
		endif
		_aVertical_5 = atn(_zLength_5/_xyLength_5)

		if	_bCutPlane_5	then
			add	moX04,	moY04,	moZ04
				rotz	180 - _aHorizon_5
					roty	-_aVertical_5
						addz	cutLct_5
							rotx	90 - aCut_5
								cutplane	1,	1,	0,	0	:	_nCutPlane = _nCutPlane + 1
								!gosub	"testPlane"
							del 1
						del 1
					del 1
				del 1
			del 1
		endif
	del 1

return


!-----------------------------------------------------------------------------------------------
"cutHotspot":
!-----------------------------------------------------------------------------------------------
	addz	_dumpValue
		!	1st
		rotz	_aHorizon_1
			roty	-_aVertical_1
				addx	_xyzLength_1/2
					addz	cutLct_1
						hotspot	0,	0,					_hPos,				unID,	aCut_1,	4 + 128	:	unID = unID + 1
						hotspot	0,	_hPos*sin(aCut_1),	_hPos*cos(aCut_1),	unID,	aCut_1,	5		:	unID = unID + 1
						hotspot	0,	0,					0,					unID,	aCut_1,	6		:	unID = unID + 1
						hotspot	1,	0,					0,					unID,	aCut_1,	7		:	unID = unID + 1
					del 1
				del 1
			del 1
		del 1

		!	2nd
		add	moX01,	moY01,	moZ01
			rotz	180 - _aHorizon_2
				roty	-_aVertical_2
					addx	_xyzLength_2/2
						addz	cutLct_2
							hotspot	0,	0,					_hPos,				unID,	aCut_2,	4 + 128	:	unID = unID + 1
							hotspot	0,	_hPos*sin(aCut_2),	_hPos*cos(aCut_2),	unID,	aCut_2,	5		:	unID = unID + 1
							hotspot	0,	0,					0,					unID,	aCut_2,	6		:	unID = unID + 1
							hotspot	1,	0,					0,					unID,	aCut_2,	7		:	unID = unID + 1
						del 1
					del 1
				del 1
			del 1
		del 1

		!	3rd
		add	moX02,	moY02,	moZ02
			rotz	180 - _aHorizon_3
				roty	-_aVertical_3
