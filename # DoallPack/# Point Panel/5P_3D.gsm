unID = 1000

hotspot	0,	0,	0

!-----------------------------------------------------------------------------------------------
!	2D Display
!-----------------------------------------------------------------------------------------------
if	bSymbol	then
	_n2Dcut = 0
	if	iProjectTo = PROJECTTO_FLOORPLAN | (iProjectTo = PROJECTTO_ABSOLUTE & btmPlaneHgt < cutPlaneHgt)	then
		addz	btmPlaneHgt
			cutplane{2}	180,	2	:	_n2Dcut = _n2Dcut + 1
		del 1
	endif

	addz	cutPlaneHgt
		cutplane{2}	180*(iProjectType = 2),	1	:	_n2Dcut = _n2Dcut + 1
	del 1

	if	iProjectTo = PROJECTTO_FLOORPLAN | (iProjectTo = PROJECTTO_ABSOLUTE & btmPlaneHgt < cutPlaneHgt)	then
		addz	topPlaneHgt
			cutplane{2}	0,	2	:	_n2Dcut = _n2Dcut + 1
		del 1
	endif
endif

!-----------------------------------------------------------------------------------------------
!	5Point Panel
!-----------------------------------------------------------------------------------------------
tempV = 0.03
tempP = max((moY01 + tempV),	(moY02 + tempV),	(moY03 + tempV),	(moY04 + tempV))

roty	-90
	x1 = tempP*cos(xang)
	y1 = tempP*sin(xang)
	
	hotspot	tempP,	0,	0,	unID,	xang,	4 + 128	:	unID = unID + 1
	hotspot	x1,		y1,	0,	unID,	xang,	5		:	unID = unID + 1
	hotspot	0,		0,	0,	unID,	xang,	6		:	unID = unID + 1	
	hotspot	0,		0,	1,	unID,	xang,	7		:	unID = unID + 1
del 1

rotx	-xang
	rotx	90
		pen			gs_3D_pen
		material	gs_mat
		
		if m_3d_type = "Volume" then
			!	Hotspot
			hotspot	0,	0,	-depth
			gosub	"movingHotspot"
			addz	-depth
				gosub	"movingHotspot"
			del 1

			!	Shape
			put	0,		0,		15,
				moX01,	moY01,	15,
				moX02,	moY02,	15,
				moX03,	moY03,	15,
				moX04,	moY04,	15,
				0,		0,		15

			prism_	nsp/3,	-depth,	get(nsp)
		endif

		if	m_3d_type = "Volume(+Z)"	then
			!	Hotspot
			hotspot	0,	0,	-depth
			gosub	"movingHotspotZ"

			!	Shape
			_dumpValue = 1.000
			group	"gVolume_Solid_1"
				pen			gs_3D_pen
				material	gs_mat

				!	하부
				put	0,		0,		0,
					moX01,	moY01,	0,
					moX02,	moY02,	0

				!	상부
				put	0,		0,		_dumpValue,
					moX01,	moY01,	(_dumpValue + moZ01),
					moX02,	moY02,	(_dumpValue + moZ02)

				ruled	(nsp/3)/2,	1+2+4+16+32,	get(nsp)
			endgroup

			group	"gVolume_Solid_2"
				pen			gs_3D_pen
				material	gs_mat

				!	하부
				put	0,		0,		0,
					moX02,	moY02,	0,
					moX03,	moY03,	0

				!	상부
				put	0,		0,		_dumpValue,
					moX02,	moY02,	(_dumpValue + moZ02),
					moX03,	moY03,	(_dumpValue + moZ03)

				ruled	(nsp/3)/2,	1+2+4+16+32,	get(nsp)
			endgroup

			group	"gVolume_Solid_3"
				pen			gs_3D_pen
				material	gs_mat

				!	하부
				put	0,		0,		0,
					moX03,	moY03,	0,
					moX04,	moY04,	0

				!	상부
				put	0,		0,		_dumpValue,
					moX03,	moY03,	(_dumpValue + moZ03),
					moX04,	moY04,	(_dumpValue + moZ04)

				ruled	(nsp/3)/2,	1+2+4+16+32,	get(nsp)
			endgroup

			group	"gVolume_Base_1"
				pen			gs_3D_pen
				material	gs_mat

				!	하부
				put	0,		0,		0,
					moX01,	moY01,	0,
					moX02,	moY02,	0

				!	상부
				put	0,		0,		(_dumpValue + depth),
					moX01,	moY01,	(_dumpValue + depth + moZ01),
					moX02,	moY02,	(_dumpValue + depth + moZ02)

				ruled	(nsp/3)/2,	1+2+4+16+32,	get(nsp)
			endgroup

			group	"gVolume_Base_2"
				pen			gs_3D_pen
				material	gs_mat

				!	하부
				put	0,		0,		0,
					moX02,	moY02,	0,
					moX03,	moY03,	0

				!	상부
				put	0,		0,		(_dumpValue + depth),
					moX02,	moY02,	(_dumpValue + depth + moZ02),
					moX03,	moY03,	(_dumpValue + depth + moZ03)

				ruled	(nsp/3)/2,	1+2+4+16+32,	get(nsp)
			endgroup

			group	"gVolume_Base_3"
				pen			gs_3D_pen
				material	gs_mat

				!	하부
				put	0,		0,		0,
					moX03,	moY03,	0,
					moX04,	moY04,	0

				!	상부
				put	0,		0,		(_dumpValue + depth),
					moX03,	moY03,	(_dumpValue + depth + moZ03),
					moX04,	moY04,	(_dumpValue + depth + moZ04)

				ruled	(nsp/3)/2,	1+2+4+16+32,	get(nsp)
			endgroup

			mulz	-1
				addz	-_dumpValue

					!	Cut_1
					group	"gPiece1"
						_bCutPlane_1 = 1
						_bCutPlane_2 = 1
						_bCutPlane_3 = 0
						_bCutPlane_4 = 0
						_bCutPlane_5 = 0

						gosub	"cutPlane"

						placegroup	subgroup("gVolume_Base_1",	"gVolume_Solid_1")

						!	Cut end
						if	_nCutPlane > eps	then
							for	c = 1	to	_nCutPlane
								cutend
							next	c
						endif
					endgroup

					!	Cut_2
					group	"gPiece2"
						_bCutPlane_1 = 1
						_bCutPlane_2 = 0
						_bCutPlane_3 = 1
						_bCutPlane_4 = 0
						_bCutPlane_5 = 1

						gosub	"cutPlane"

						placegroup	subgroup("gVolume_Base_2",	"gVolume_Solid_2")

						!	Cut end
						if	_nCutPlane > eps	then
							for	c = 1	to	_nCutPlane
								cutend
							next	c
						endif
					endgroup

					!	Cut_3
					group	"gPiece3"
						_bCutPlane_1 = 0
						_bCutPlane_2 = 0
						_bCutPlane_3 = 0
						_bCutPlane_4 = 1
						_bCutPlane_5 = 1

						gosub	"cutPlane"

						placegroup	subgroup("gVolume_Base_3",	"gVolume_Solid_3")

						!	Cut end
						if	_nCutPlane > eps	then
							for	c = 1	to	_nCutPlane
								cutend
							next	c
						endif
					endgroup

					!	Hotspot
					gosub	"cutHotspot"

					!	Place
					addPiece_1 = addgroup("gPiece1",	"gPiece2")
					addPiece_2 = addgroup(addPiece_1,	"gPiece3")

					placegroup	addPiece_2

					! placegroup	"gPiece1"

				del 1
			del 1
		endif

		if	m_3d_type = "Surface"	then
			!	Hotspot
			hotspot	0,	0,	-depth
			gosub	"movingHotspot"

			!	Shape
			put	0,		0,		0,
				moX01,	moY01,	0,
				moX02,	moY02,	0,
				moX03,	moY03,	0,
				moX04,	moY04,	0,
				0,		0,		0

			plane	nsp/3,	get(nsp)
		endif
	
	del 1
	
del 1

!-----------------------------------------------------------------------------------------------
!	2D Display
!-----------------------------------------------------------------------------------------------
if	bSymbol	then
	for	i = 1	to	_n2Dcut
		cutend
	next	i
endif



end



