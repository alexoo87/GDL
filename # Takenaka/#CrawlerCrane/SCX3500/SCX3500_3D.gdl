		_ConnectThk = 1.150

		addz	_zPosConnect
			put	-1.5825,	-3.125,	15,
				-1.5825,	-2.665,	15,
				-1.4925,	-2.575,	15,
				-1.4925,	2.575,	15,
				-1.5825,	2.665,	15,
				-1.5825,	3.125,	15,
				-0.9625,	3.125,	15,
				-0.9625,	2.665,	15,
				-0.2375,	2.470,	15,
