					3.920,	-1.530,	15,
					1.495,	-1.530,	15

				prism_	nsp/3,	_mainDeckThk,	get(nsp)
			del 1

			!	Main Object
			_mainDpt = 2.970

			addy	_mainDpt/2
				rotx	90
					put	1.500,	0,			15,
						-6.250,	0,			15,
						-6.250,	1.405,		15,
						0,		1.405,		15,
						1.178,	1.325,		15,
						1.530,	1.257,		13,
