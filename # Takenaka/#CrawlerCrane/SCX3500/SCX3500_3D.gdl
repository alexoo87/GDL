				1.5825,		-3.125,	15,
				0.9625,		-3.125,	15,
				0.9625,		-2.665,	15,
				0.2375,		-2.470,	15,
				-0.2375,	-2.470,	15,
				-0.9625,	-2.665,	15,
				-0.9625,	-3.125,	15

