																							bMirror_Strut_1 = 0,
																							distStrut_SP = 1.000,
																							distStrut_EP = 1.000,
																							strutLth_1 = 11.300,
																							strutThk = 0.025,
																							strutRad_1 = 0.210
										del 1
									del 1
								del 1
							endif
						del 1
					del 1
				del 1
			del 1
		del 1

	! !	Wire
	! !-----------------------------------------------------------------------------------------------
