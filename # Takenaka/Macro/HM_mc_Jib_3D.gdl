							put	0,		-0.085,	15,
								0,		0.085,	15,
								0.020,	0.085,	15,
								0.0805,	0.072,	15,
								0.0805,	-0.072,	15,
								0.020,	-0.085,	15

							prism_	nsp/3,	_brgWdt,	get(nsp)
						del 1
					del 1
				del 1

				!	Anchor Bar
				_barRad = 0.025
				_barLth = 0.274*2

				addx	_xPosAnc + 0.140
					rotx	90
						addz	-_barLth/2
							cylind	_barLth,	_barRad
						del 1
					del 1
				del 1

				!	Anchor Wheel
				_innerSRad = 0.0985
				_innerSWdt = 0.021
				_innerLRad = 0.1095
				_innerLWdt = 0.0055
				_outerRad = 0.065
				_outerWdt = 0.009

				addx	_xPosAnc + 0.140
