							endif
						del	1
					del	1
				del	1

				!	Top
				addz	_mOutFrmD_start
				 	roty	-90 - _mOutFrmAng_top
						addz	-_mAdd/2
							if	not(GLOB_FEEDBACK_MODE)	then
								cylind _mOutFrmL+_mAdd, _mOutFrmR
							else
