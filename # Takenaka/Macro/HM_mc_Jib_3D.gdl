					roty	30*i
						addz	-_cntRad
							cutplane	1,	1,	0,	1
						del 1
					del 1
				next	i

				add	0.400 - 0.0045,	0,	-_zPosCnt
					roty	_aCut
						cutplane	1,	1,	0,	1
					del 1
				del 1
				add	0.400 - 0.0045,	0,	_zPosCnt
					roty	-_aCut
						cutplane	1,	1,	0,	0
					del 1
				del 1

					!	Connection
