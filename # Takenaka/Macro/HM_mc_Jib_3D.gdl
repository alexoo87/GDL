							-_msrHgt/2,	-_msrWdt/2,	-1,
							0,			0,			900,
							_cydRad,	360,		4000+15

						prism_	nsp/3,	_msrLth,	get(nsp)

						add	-0.105,	-_msrWdt/2,	0.260
							rotx	90
								!	Back
								material	_stMat_White
								put	0.150,	0,		15,
