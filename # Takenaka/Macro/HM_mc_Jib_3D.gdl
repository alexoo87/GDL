								gosub	"anchorWheel_"
							del 1
						next	i
					del 1

					gosub	"anchorWheel_"

					addy	-0.140
						for	i = 1	to	3
							addy	-0.052*(i - 1)
								gosub	"anchorWheel_"
							del 1
						next	i
					del 1
				del 1
